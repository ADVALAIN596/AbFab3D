importPackage(Packages.abfab3d.grid.op)
importPackage(Packages.abfab3d.grid)

grid = load("models/sphere.stl.gz")
model = new DataSourceGrid(grid, 63)
intersect = new DataSources.Intersection();
intersect.addDataSource(model)
intersect.addDataSource(new VolumePatterns.Gyroid(10*MM, 1*MM));

maker.setDataSource(intersect);
maker.setMaxAttributeValue(63);

dest = createGrid(grid);
maker.makeGrid(dest);
show(dest)

