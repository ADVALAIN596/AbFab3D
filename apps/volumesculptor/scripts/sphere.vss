importPackage(Packages.abfab3d.grid.op);
importPackage(Packages.abfab3d.grid);
importPackage(Packages.abfab3d.datasources);

function main(radius) {
	var grid = createGrid(-25*MM,25*MM,-25*MM,25*MM,-25*MM,25*MM,.1*MM);
	var sphere = new Sphere(radius);
	maker.setSource(sphere);
	maker.makeGrid(grid);
	
	return grid;
}